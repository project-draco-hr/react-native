{
  if (properties.isEmpty()) {
    return CodeBlock.builder().addStatement("mappings = $T.emptyMap()",Collections.class).build();
  }
  CodeBlock.Builder builder=CodeBlock.builder().addStatement("mappings = new $T($L)",CONCRETE_MAPPINGS_MAP_TYPE,properties.size());
  for (int i=0, size=properties.size(); i < size; i++) {
    PropertyInfo propertyInfo=properties.get(i);
    builder.addStatement("mappings.put($S, $L)",propertyInfo.mProperty.name(),i);
  }
  return builder.build();
}
