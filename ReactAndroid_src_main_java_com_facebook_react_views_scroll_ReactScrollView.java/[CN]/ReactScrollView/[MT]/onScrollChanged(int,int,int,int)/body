{
  super.onScrollChanged(x,y,oldX,oldY);
  if (mOnScrollDispatchHelper.onScrollChanged(x,y)) {
    dockClosestSectionHeader();
    if (mRemoveClippedSubviews) {
      updateClippingRect();
    }
    if (mFlinging) {
      mDoneFlinging=false;
    }
    ReactScrollViewHelper.emitScrollEvent(this);
  }
}
