{
  PowerMockito.mockStatic(Arguments.class);
  Mockito.when(Arguments.createArray()).thenAnswer(new Answer<Object>(){
    @Override public Object answer(    InvocationOnMock invocation) throws Throwable {
      return new SimpleArray();
    }
  }
);
  Mockito.when(Arguments.createMap()).thenAnswer(new Answer<Object>(){
    @Override public Object answer(    InvocationOnMock invocation) throws Throwable {
      return new SimpleMap();
    }
  }
);
  mStorage=new AsyncStorageModule(new ReactApplicationContext(new FakeFragmentContext(RuntimeEnvironment.application)));
  mEmptyArray=new SimpleArray();
}
