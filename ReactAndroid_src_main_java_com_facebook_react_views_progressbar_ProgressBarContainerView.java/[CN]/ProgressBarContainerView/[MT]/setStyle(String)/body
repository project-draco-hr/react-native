{
  int style=ReactProgressBarViewManager.getStyleFromString(styleName);
  mProgressBar=new ProgressBar(getContext(),null,style);
  mProgressBar.setMax(MAX_PROGRESS);
  removeAllViews();
  addView(mProgressBar,new ViewGroup.LayoutParams(ViewGroup.LayoutParams.MATCH_PARENT,ViewGroup.LayoutParams.MATCH_PARENT));
}
