{
  return MapBuilder.builder().put(ScrollEvent.EVENT_NAME,MapBuilder.of("registrationName","onScroll")).put("topScrollBeginDrag",MapBuilder.of("registrationName","onScrollBeginDrag")).put("topScrollEndDrag",MapBuilder.of("registrationName","onScrollEndDrag")).put("topScrollAnimationEnd",MapBuilder.of("registrationName","onScrollAnimationEnd")).put("topMomentumScrollBegin",MapBuilder.of("registrationName","onMomentumScrollBegin")).put("topMomentumScrollEnd",MapBuilder.of("registrationName","onMomentumScrollEnd")).build();
}
