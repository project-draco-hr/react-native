{
  super(reactContext);
  mViewManagers=new ViewManagerRegistry(viewManagerList);
  mEventDispatcher=new EventDispatcher(reactContext);
  mNativeViewHierarchyManager=new NativeViewHierarchyManager(mViewManagers);
  mOperationsQueue=new UIViewOperationQueue(reactContext,mNativeViewHierarchyManager);
  mNativeViewHierarchyOptimizer=new NativeViewHierarchyOptimizer(mOperationsQueue,mShadowNodeRegistry);
  DisplayMetrics displayMetrics=reactContext.getResources().getDisplayMetrics();
  DisplayMetricsHolder.setDisplayMetrics(displayMetrics);
  mModuleConstants=createConstants(displayMetrics,viewManagerList);
  reactContext.addLifecycleEventListener(this);
}
