{
  if (mUseDeveloperSupport) {
    if (mDevSupportManager.hasUpToDateJSBundleInCache()) {
      onJSBundleLoadedFromServer();
      return;
    }
 else     if (mBundleAssetName == null || !mDevSupportManager.hasBundleInAssets(mBundleAssetName)) {
      mDevSupportManager.handleReloadJS();
      return;
    }
  }
  recreateReactContextInBackground(new JSCJavaScriptExecutor(),JSBundleLoader.createAssetLoader(mApplicationContext.getAssets(),mBundleAssetName));
}
