{
  if (!Float.isNaN(node.layout.dimensions[dim[axis]])) {
    return;
  }
  if (Float.isNaN(node.style.dimensions[dim[axis]]) || node.style.dimensions[dim[axis]] <= 0.0) {
    return;
  }
  float maxLayoutDimension=Math.max(boundAxis(node,axis,node.style.dimensions[dim[axis]]),node.style.padding.getWithFallback(leadingSpacing[axis],leading[axis]) + node.style.padding.getWithFallback(trailingSpacing[axis],trailing[axis]) + node.style.border.getWithFallback(leadingSpacing[axis],leading[axis])+ node.style.border.getWithFallback(trailingSpacing[axis],trailing[axis]));
  node.layout.dimensions[dim[axis]]=maxLayoutDimension;
}
