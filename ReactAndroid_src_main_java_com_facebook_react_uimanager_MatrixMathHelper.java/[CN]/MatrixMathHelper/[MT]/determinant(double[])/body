{
  double m00=matrix[0], m01=matrix[1], m02=matrix[2], m03=matrix[3], m10=matrix[4], m11=matrix[5], m12=matrix[6], m13=matrix[7], m20=matrix[8], m21=matrix[9], m22=matrix[10], m23=matrix[11], m30=matrix[12], m31=matrix[13], m32=matrix[14], m33=matrix[15];
  return (m03 * m12 * m21* m30 - m02 * m13 * m21* m30 - m03 * m11 * m22* m30 + m01 * m13 * m22* m30 + m02 * m11 * m23* m30 - m01 * m12 * m23* m30 - m03 * m12 * m20* m31 + m02 * m13 * m20* m31 + m03 * m10 * m22* m31 - m00 * m13 * m22* m31 - m02 * m10 * m23* m31 + m00 * m12 * m23* m31 + m03 * m11 * m20* m32 - m01 * m13 * m20* m32 - m03 * m10 * m21* m32 + m00 * m13 * m21* m32 + m01 * m10 * m23* m32 - m00 * m11 * m23* m32 - m02 * m11 * m20* m33 + m01 * m12 * m20* m33 + m02 * m10 * m21* m33 - m00 * m12 * m21* m33 - m01 * m10 * m22* m33 + m00 * m11 * m22* m33);
