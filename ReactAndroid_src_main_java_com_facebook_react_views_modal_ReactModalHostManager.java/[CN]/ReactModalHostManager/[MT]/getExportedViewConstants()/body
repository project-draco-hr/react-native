{
  final int heightResId=mContext.getResources().getIdentifier("status_bar_height","dimen","android");
  final float height=heightResId > 0 ? PixelUtil.toDIPFromPixel(mContext.getResources().getDimensionPixelSize(heightResId)) : 0;
  return MapBuilder.<String,Object>of("StatusBarHeight",height);
}
