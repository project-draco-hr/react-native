{
  mContainsImages=update.containsImages();
  if (getLayoutParams() == null) {
    setLayoutParams(EMPTY_LAYOUT_PARAMS);
  }
  setText(update.getText());
  Spacing padding=update.getPadding();
  setPadding((int)Math.ceil(padding.get(Spacing.LEFT)),(int)Math.ceil(padding.get(Spacing.TOP)),(int)Math.ceil(padding.get(Spacing.RIGHT)),(int)Math.ceil(padding.get(Spacing.BOTTOM)));
}
