{
  mContainsImages=update.containsImages();
  if (getLayoutParams() == null) {
    setLayoutParams(EMPTY_LAYOUT_PARAMS);
  }
  setText(update.getText());
  setPadding((int)Math.ceil(update.getPaddingLeft()),(int)Math.ceil(update.getPaddingTop()),(int)Math.ceil(update.getPaddingRight()),(int)Math.ceil(update.getPaddingBottom()));
}
